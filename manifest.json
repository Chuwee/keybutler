{
    "manifest_version": 3,
    "name": "Keyholder",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["generator.js", "content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  