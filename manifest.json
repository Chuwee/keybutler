{
    "manifest_version": 3,
    "name": "Algorithm Editor",
    "version": "1.0",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "js": [
        "generator.js",
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
